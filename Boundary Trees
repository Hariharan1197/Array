#include<stdio.h>
#include <stdlib.h>

int main()
{
int r,c,i,j;
scanf("%d %d",&r,&c);
int a[r][c];
for(i=0;i<r;i++)
{
    for(j=0;j<c;j++)
    {
        scanf("%d",&a[i][j]);
    }
}
int g,m;
scanf("%d %d",&g,&m);
int mid,cor;
mid=g*m;
cor=2*mid;
for(i=0;i<r;i++)
{
    for(j=0;j<c;j++)
    {
        if(i==0 || j==0 || i==r-1 || j==c-1)
        {
            printf("%d ",a[i][j]+cor);
        }
        else
        {
            printf("%d ",a[i][j]+mid);
        }
    }
    printf("\n");
}

}
