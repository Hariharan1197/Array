#include<stdio.h>
#include <stdlib.h>

int main()
{
int a,c,e=0;
scanf("%d",&a);
int b[a],d[10];
for(int i=1;i<=a;i++)
{
scanf("%d",&b[i]);
}    
scanf("%d",&c);
for(int i=1;i<=a;i++)
{
    if(b[i]==c)
    {
        d[e]=i;
        e++;
    }
}
if(e==0)
{
    printf("-1 -1");
}
else
{
    printf("%d %d",d[0],d[e-1]);
}
}
